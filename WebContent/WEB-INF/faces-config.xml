<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<message-bundle>message-bundle</message-bundle>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
	</application>
	<converter>
		<converter-id>JogadorConverter</converter-id>
		<converter-class>net.barragem.view.converter.JogadorConverter</converter-class>
	</converter>
	<converter>
		<converter-id>JogadorEventoConverter</converter-id>
		<converter-class>net.barragem.view.converter.JogadorEventoConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>lookupBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.LookupBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gerirJogadorBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.GerirJogadorBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.LoginBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gerirBarragemBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.GerirBarragemBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gerirCicloBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.GerirCicloBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gerirRodadaBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.GerirRodadaBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>gerirJogoBarragemBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.GerirJogoBarragemBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>messageBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.MessageBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>registrarNovoUsuarioBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.RegistrarNovoUsuarioBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>arquivoBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.ArquivoBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>editarPerfilBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.EditarPerfilBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>exibirPainelBarragemBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.ExibirPainelBarragemBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>exibirPerfilBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.ExibirPerfilBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>pesquisarBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.PesquisarBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>vincularJogadorBean</managed-bean-name>
		<managed-bean-class>net.barragem.view.backingbean.VincularJogadorBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>protectedpages/gerirbarragem/gerirBarragem.xhtml</display-name>
		<from-view-id>/protectedpages/gerirbarragem/gerirBarragem.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editarCiclo</from-outcome>
			<to-view-id>/protectedpages/gerirciclo/gerirCiclo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>exibirPainelBarragem</from-outcome>
			<to-view-id>/protectedpages/exibirpainelbarragem/exibirPainelBarragem.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>protectedpages/gerirciclo/gerirCiclo.xhtml</display-name>
		<from-view-id>/protectedpages/gerirciclo/gerirCiclo.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editarRodada</from-outcome>
			<to-view-id>/protectedpages/gerirrodada/gerirRodada.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editarCiclo</from-outcome>
			<to-view-id>/protectedpages/gerirciclo/gerirCiclo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>protectedpages/gerirrodada/gerirRodada.xhtml</display-name>
		<from-view-id>/protectedpages/gerirrodada/gerirRodada.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>editarCiclo</from-outcome>
			<to-view-id>/protectedpages/gerirciclo/gerirCiclo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editarRodada</from-outcome>
			<to-view-id>/protectedpages/gerirrodada/gerirRodada.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>passo1</from-outcome>
			<to-view-id>/publicpages/novousuario/passo-1-seguranca.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>esqueciMinhaSenha</from-outcome>
			<to-view-id>/publicpages/esqueciMinhaSenha.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>publicpages/novousuario/passo-1-seguranca.xhtml</display-name>
		<from-view-id>/publicpages/novousuario/passo-1-seguranca.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>passo0</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>passo2</from-outcome>
			<to-view-id>/protectedpages/novousuario/passo-2-perfil.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>protectedpages/novousuario/passo-2-perfil.xhtml</display-name>
		<from-view-id>/protectedpages/novousuario/passo-2-perfil.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>passo3</from-outcome>
			<to-view-id>/protectedpages/novousuario/passo-3-foto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>protectedpages/novousuario/passo-3-foto.xhtml</display-name>
		<from-view-id>/protectedpages/novousuario/passo-3-foto.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>fim</from-outcome>
			<to-view-id>/protectedpages/index.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>passo2</from-outcome>
			<to-view-id>/protectedpages/novousuario/passo-2-perfil.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/protectedpages/gerirjogador/gerirJogador.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>sucessoPesquisa</from-outcome>
			<to-view-id>/protectedpages/pesquisa/resultadoPesquisaJogador.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/protectedpages/*</from-view-id>
		<navigation-case>
			<from-outcome>exibirPainelBarragem</from-outcome>
			<to-view-id>/protectedpages/exibirpainelbarragem/exibirPainelBarragem.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>logout</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>exibirPerfil</from-outcome>
			<to-view-id>/protectedpages/exibirperfil/exibirPerfil.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editarDadosBasicos</from-outcome>
			<to-view-id>/protectedpages/editarperfil/editarDadosBasicos.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>editarPerfil</from-outcome>
			<to-view-id>/protectedpages/editarperfil/editarPerfil.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editarFoto</from-outcome>
			<to-view-id>/protectedpages/editarperfil/editarFoto.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/protectedpages/index.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>
	<lifecycle>
		<phase-listener>net.barragem.scaffold.CacheControlPhaseListener</phase-listener>
	</lifecycle>
</faces-config>